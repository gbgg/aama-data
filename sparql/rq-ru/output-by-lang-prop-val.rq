PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX aama: <http://oi.uchicago.edu/aama/schema/2010#>

SELECT DISTINCT ?lang ?langVar ?property ?value 
WHERE 
{
   ?s ?p ?o .
   ?s aama:lang ?language .
   ?language rdfs:label ?lang .
   #&?s aama:langVar ?languageVar .
	#&?languageVar rdfs:label ?langVar . 
   ?p rdfs:label ?property .
   ?o rdfs:label ?value
}
ORDER BY ?lang ?langVar ?property ?value 
