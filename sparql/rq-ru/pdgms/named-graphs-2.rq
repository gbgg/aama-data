PREFIX rdf:	 <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs:	 <http://www.w3.org/2000/01/rdf-schema#>
PREFIX aama:	 <http://id.oi.uchicago.edu/aama/2013/>
PREFIX aamas:	 <http://id.oi.uchicago.edu/aama/2013/schema/>
PREFIX aamag:	 <http://id.oi.uchicago.edu/aama/2013/graph/>
PREFIX orm:   <http://id.oi.uchicago.edu/aama/2013/oromo/>

SELECT DISTINCT  ?proplabel ?superprop ?valuelabel
WHERE
{
  GRAPH ?g 
  { 
		?s ?prop ?value .
		?prop rdfs:subPropertyOf ?superprop .
		?value rdfs:label ?valuelabel .
		?prop rdfs:label ?proplabel .
		#?superprop rdfs:label ?superproplabel .
                #?g rdfs:label ?glabel .
	}
}
ORDER BY ?g ?proplabel ?superprop ?valuelabel