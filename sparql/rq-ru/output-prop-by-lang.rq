PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX aama: <http://oi.uchicago.edu/aama/schema/2010#>

SELECT DISTINCT ?proplabel ?lang ?langVar
WHERE 
{
   ?s ?p ?o .
   ?s aama:lang ?language .
   ?language rdfs:label ?lang .
   #&?s aama:langVar ?languageVar .
	#&?languageVar rdfs:label ?langVar . 
   ?p rdfs:label ?proplabel .

   }
ORDER BY ?proplabel ?lang ?langVar


