PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX aama: <http://oi.uchicago.edu/aama/schema/2010#>

SELECT DISTINCT ?pdgmlabel ?prop1 ?val1 
WHERE 
{
    ?pid ?term aama:pid ;
	aama:pdgmlabel ?pdgmlabel .
	?s aama:pid ?pid ;
	#aama:lang aama:LANG ;
	#aama:langVar aama:LANGVAR ;
 	?p ?o .
   ?p rdfs:label ?prop1 .
   ?o rdfs:label ?val1  .
	FILTER (?p NOT IN ( aama:lang, aama:langVar))
	 
}
ORDER BY ?pid ?prop1

