PREFIX rdf:	 <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs:	 <http://www.w3.org/2000/01/rdf-schema#>
PREFIX aama:	 <http://id.oi.uchicago.edu/aama/2013/>
PREFIX aamas:	 <http://id.oi.uchicago.edu/aama/2013/schema/>
PREFIX aamag:	 <http://id.oi.uchicago.edu/aama/2013/graph/>
PREFIX orm:   <http://id.oi.uchicago.edu/aama/2013/oromo/>
PREFIX bar:   <http://id.oi.uchicago.edu/aama/2013/beja-arteiga/> 
PREFIX bat:   <http://id.oi.uchicago.edu/aama/2013/beja-atmaan/> 
		
SELECT DISTINCT  ?graph ?language ?value ?property
WHERE
{
  GRAPH ?graph
  { 
		?val rdf:type aamas:Present .
		?val rdfs:label ?value .
		?val aamas:lang ?lang .
		?prop rdfs:subPropertyOf aamas:tam .
		?prop rdfs:range aamas:Present .
		?val rdfs:label ?value .
		?prop rdfs:label ?property .
		?lang rdfs:label ?language .
	}
}
