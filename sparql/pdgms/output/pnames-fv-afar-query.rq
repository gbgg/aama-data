# Yields properties that cooccur with finite verbs. Edited list is in pdgm-finite-props.txt
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX aama: <http://oi.uchicago.edu/aama/schema/2010#>
PREFIX aamas:	 <http://id.oi.uchicago.edu/aama/2013/schema/>
PREFIX aamag:	 <http://oi.uchicago.edu/aama/2013/graph/>
PREFIX afr: <http://id.oi.uchicago.edu/aama/2013/afar/>

SELECT DISTINCT  ?property ?lexeme
WHERE {
GRAPH <http://oi.uchicago.edu/aama/2013/graph/afar> {
	?s ?p ?o ;
	#aamas:lexeme ?lexeme;
    #afr:tam  ?tam ;
	afr:pos  afr:Verb ;
	afr:person ?person .
   ?p rdfs:label ?property .
 	FILTER (?p NOT IN ( aamas:lang, afr:gender, afr:number, afr:person, afr:pos, afr:token, rdf:type, afr:multiLex ) )
# 	FILTER (?p NOT IN ( aamas:lang, afr:gender, afr:number, afr:person, afr:pos, afr:token, rdf:type) )
}}
ORDER BY ASC(?property) ASC(?lexeme)

