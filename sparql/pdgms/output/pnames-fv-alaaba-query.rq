# Yields properties that cooccur with finite verbs. Edited list is in pdgm-finite-props.txt
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX aama: <http://oi.uchicago.edu/aama/schema/2010#>
PREFIX aamas:	 <http://id.oi.uchicago.edu/aama/2013/schema/>
PREFIX aamag:	 <http://oi.uchicago.edu/aama/2013/graph/>
PREFIX alb: <http://id.oi.uchicago.edu/aama/2013/alaaba/>

SELECT DISTINCT  ?property ?lexeme
WHERE {
GRAPH <http://oi.uchicago.edu/aama/2013/graph/alaaba> {
	?s ?p ?o ;
	#aamas:lexeme ?lexeme;
    #alb:tam  ?tam ;
	alb:pos  alb:Verb ;
	alb:person ?person .
   ?p rdfs:label ?property .
 	FILTER (?p NOT IN ( aamas:lang, alb:gender, alb:number, alb:person, alb:pos, alb:token, rdf:type, alb:multiLex ) )
# 	FILTER (?p NOT IN ( aamas:lang, alb:gender, alb:number, alb:person, alb:pos, alb:token, rdf:type) )
}}
ORDER BY ASC(?property) ASC(?lexeme)

