# Not that different from properties.template. Cf. LS ex445.rq
prefix aama:	 <http://id.oi.uchicago.edu/aama/2013/>
prefix aamas:	 <http://id.oi.uchicago.edu/aama/2013/schema/>
prefix rdf:	 <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs:	 <http://www.w3.org/2000/01/rdf-schema#>

SELECT DISTINCT ?property ?subProperty 
WHERE {
GRAPH <http://oi.uchicago.edu/aama/2013/graph/%lang%> {
	   ?subProperty rdfs:subPropertyOf+ rdfs:Property .
	  ?subProperty rdfs:subPropertyOf ?property
	  }
}
ORDER BY DESC(?property) ?subProperty
