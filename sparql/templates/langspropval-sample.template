# For qstring: "person=Person2,gender=Fem,number=?number,pos=?pos"
#in what languages is there a 2f, and if so, in what num and what pos?

prefix aama:	 <http://id.oi.uchicago.edu/aama/2013/>
prefix aamas:	 <http://id.oi.uchicago.edu/aama/2013/schema/>
prefix rdf:	 <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs:	 <http://www.w3.org/2000/01/rdf-schema#>

SELECT DISTINCT  ?language   ?valProp3Label ?valProp4Label
WHERE {
GRAPH <http://oi.uchicago.edu/aama/2013/graph/%lang%> {
	?s ?o ?p ;
	<http://id.oi.uchicago.edu/aama/2013/%lang%/%prop1%> <http://id.oi.uchicago.edu/aama/2013/%lang%/%val1%> ;
	<http://id.oi.uchicago.edu/aama/2013/%lang%/%prop2%> <http://id.oi.uchicago.edu/aama/2013/%lang%/%val2%> ;
	<http://id.oi.uchicago.edu/aama/2013/%lang%/%prop3%> ?valProp3 ;
	<http://id.oi.uchicago.edu/aama/2013/%lang%/%prop4%> ?valProp4 ;
	aamas:lang ?lang .
	?lang rdfs:label ?language .
	?valProp3 rdfs:label ?valProp3Label .
    ?valProp4 rdfs:label ?valProp4Label .
}}
ORDER BY ?language   ?valProp3Label ?valProp4Label

