prefix aama:	 <http://id.oi.uchicago.edu/aama/2013/>
prefix aamas:	 <http://id.oi.uchicago.edu/aama/2013/schema/>
prefix rdf:	 <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs:	 <http://www.w3.org/2000/01/rdf-schema#>

SELECT DISTINCT ?language ?lexlabel ?lemma
WHERE {
GRAPH <http://oi.uchicago.edu/aama/2013/graph/%lang%> {
	  ?pred aamas:lang ?lang .
	  ?lang rdfs:label ?language .
      ?pred rdf:type aamas:Lexeme .
      ?pred rdfs:label ?lexlabel .
	  OPTIONAL { ?pred aamas:lemma ?lemma .}
}}
ORDER BY ASC(?lexlabel)
