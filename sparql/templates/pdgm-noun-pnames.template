# Yields properties that cooccur with pronouns. For the moment (11/26/13) presuming that all pro paradigms marked with proClass property. 

PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX aama: <http://oi.uchicago.edu/aama/schema/2010#>
PREFIX aamas:	 <http://id.oi.uchicago.edu/aama/2013/schema/>
PREFIX aamag:	 <http://oi.uchicago.edu/aama/2013/graph/>
PREFIX %abbrev%: <http://id.oi.uchicago.edu/aama/2013/%lang%/>

SELECT DISTINCT  ?morphClassLabel ?property 
WHERE {
GRAPH <http://oi.uchicago.edu/aama/2013/graph/%lang%> {
	?s ?p ?o ;
	#rdf:type aamas:Muterm ;
	%abbrev%:morphClass ?morphClass ;
	%abbrev%:pos  %abbrev%:Noun .
	?morphClass rdfs:label ?morphClassLabel .
   ?p rdfs:label ?property .
  	FILTER (?p NOT IN ( aamas:lang, aamas:muterm, %abbrev%:pos, rdf:type,  %abbrev%:token))
}}
ORDER BY ASC(?morphClassLabel) ASC(?property)
